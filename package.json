{
  "name": "Threlte Practice",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run format",
    "format": "prettier --write 'dist/**/*.html'",
    "preview": "astro preview",
    "lint": "eslint src/ --ext .astro,.ts",
    "delivery": "astro build && npm run delivery:format && npm run delivery:copy && npm run delivery:list",
    "delivery:format": "prettier --write 'dist/**/*.html'",
    "delivery:copy": "node 'scripts/copyDirectory.js'",
    "delivery:list": "node 'scripts/makeFileList.js'",
    "svgsprite": "node 'scripts/svgSprite.js'",
    "preinstall": "node scripts/initialSetting.js"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.7",
    "@astrojs/svelte": "^7.0.4",
    "@inquirer/prompts": "^7.2.3",
    "@mkkellogg/gaussian-splats-3d": "^0.4.6",
    "@threlte/core": "^8.0.1",
    "@threlte/extras": "^9.0.1",
    "@types/fs-extra": "^11.0.4",
    "astro": "^5.1.9",
    "camera-controls": "^2.9.0",
    "fs-extra": "^11.3.0",
    "sharp": "^0.33.5",
    "svelte": "^5.1.16",
    "svelte-tweakpane-ui": "^1.5.1",
    "three": "^0.172.0",
    "tree-node-cli": "^1.6.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^4.6.14",
    "@types/three": "^0.172.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "glob": "^11.0.1",
    "markuplint": "^4.11.4",
    "node-html-parser": "^7.0.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "svg-sprite": "^2.0.4"
  }
}
